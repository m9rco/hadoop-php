{
  "name": "m9rco/hadoop-php",
  "description": "map/reduce tasks for Hadoop in PHP.",
  "keywords": [
    "hadoop",
    "php",
    "sdk",
    "library"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "m9rco",
      "email": "pushaowei0727@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "kherge/box": "*",
    "pimple/pimple": "^3.0",
    "seld/phar-utils": "^1.0",
    "symfony/class-loader": "~3"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "PHPHadoop\\": "src/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload-dev": {
    "psr-4": {
      "PHPHadoop\\Tests\\": "tests/"
    }
  }
}
